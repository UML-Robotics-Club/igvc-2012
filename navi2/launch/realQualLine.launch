<!-- -*- xml -*- -->
<launch>
    <group ns="robot">
        <node name="qualLine" pkg="navi2" type="qualLine">
            <remap from="output" to="goal"/>
        </node>

        <node name="woah" pkg="woah_scheme" type="woah_scheme">
            <remap from="path" to="FilteredLocalPath"/>
            <remap from="base_scan" to="woahLaser"/>
        </node>
    </group>
</launch>

